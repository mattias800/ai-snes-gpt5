{
  "name": "ai-snes-gpt5",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run --reporter=dot",
    "test:watch": "vitest --watch",
    "test:timing": "vitest run --reporter=dot tests/ppu/timing",
    "test:watch:timing": "vitest --watch tests/ppu/timing",
    "lint": "eslint .",
    "format": "prettier -w .",
    "screenshot": "tsx scripts/headless_screenshot.ts",
    "screenshot:smw": "SMW_APU_SHIM=1 tsx scripts/headless_screenshot.ts --rom=smw.sfc --frames=1200 --out=smw_1200.png --pressStartFrame=600 --noFallback=1 --debug=1 --logMmio=1 --logMmioLimit=300",
    "smw:smoke": "tsx scripts/smw_smoke.ts",
    "setup:snes-tests": "unzip -o third_party/snes-tests/snes-tests.zip -d third_party/snes-tests/",
    "build:snes-tests": "make -C test-roms/snes-tests -j",
    "test:snes-vectors": "SNES_TESTS_DIR=test-roms/snes-tests npx vitest run --reporter=dot tests/cpu/snes_cputest_vectors_full.test.ts",
    "test:snes-vectors:full": "CPU_VECTORS_MODE=full SNES_TESTS_DIR=test-roms/snes-tests npx vitest run --reporter=dot tests/cpu/snes_cputest_vectors_full.test.ts",
    "test:apu": "vitest run --reporter=dot tests/apu",
    "test:spc-vectors": "SNES_TESTS_DIR=test-roms/snes-tests vitest run --reporter=dot tests/apu/spc700_vectors.test.ts",
    "test:spc-vectors:full": "SPC_VECTORS_LIMIT=0 SNES_TESTS_DIR=test-roms/snes-tests vitest run --reporter=dot tests/apu/spc700_vectors.test.ts",
    "test:snes-roms:cpu": "RUN_SNES_ROMS=1 SNES_TESTS_DIR=test-roms/snes-tests vitest run --reporter=dot tests/emulator/snes_tests_passfail.test.ts -t cpu-basic",
    "test:snes-roms:apu": "RUN_SNES_ROMS=1 APU_SPC700_CORE=1 SNES_TESTS_DIR=test-roms/snes-tests vitest run --reporter=dot tests/emulator/snes_tests_passfail.test.ts -t spctest",
    "test:snes-roms:all": "RUN_SNES_ROMS=1 APU_SPC700_CORE=1 SNES_TESTS_DIR=test-roms/snes-tests vitest run --reporter=dot tests/emulator/snes_tests_passfail.test.ts",
    "gen:snes-cpu-vectors": "tsx scripts/gen-snes-cpu-vectors.ts",
    "gen:snes-spc-vectors": "tsx scripts/gen-snes-spc-vectors.ts",
    "spc:render": "tsx scripts/spc_to_wav.ts",
    "web:dev": "vite web",
    "web:build": "vite build web",
    "web:preview": "vite preview web"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "fast-check": "^4.2.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^2.1.9",
    "vite": "^5.4.6"
  }
}
