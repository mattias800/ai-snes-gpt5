{
  "name": "ai-snes-gpt5",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run --reporter=dot",
    "test:watch": "vitest --watch",
    "lint": "eslint .",
    "format": "prettier -w .",
    "screenshot": "tsx scripts/headless_screenshot.ts",
    "screenshot:smw": "SMW_APU_SHIM=1 tsx scripts/headless_screenshot.ts --rom=smw.sfc --frames=1200 --out=smw_1200.png --pressStartFrame=600 --noFallback=1 --debug=1 --logMmio=1 --logMmioLimit=300",
    "smw:smoke": "tsx scripts/smw_smoke.ts",
    "setup:snes-tests": "unzip -o third_party/snes-tests/snes-tests.zip -d third_party/snes-tests/",
    "test:snes-vectors": "SNES_TESTS_DIR=third_party/snes-tests npx vitest run --reporter=dot tests/cpu/snes_cputest_vectors_full.test.ts",
    "test:snes-vectors:full": "CPU_VECTORS_MODE=full SNES_TESTS_DIR=third_party/snes-tests npx vitest run --reporter=dot tests/cpu/snes_cputest_vectors_full.test.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "fast-check": "^4.2.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^2.1.9"
  }
}
